{
	"manifest_version": 2,
	"name": "One-click logout",
	"version": "0.2.2",
	"description": "Provides one-click logout from popular services",
 	"icons": {
		"32": "ocl_32.png",
		"64": "ocl_64.png"
	},
	"permissions": [
		"https://accounts.google.com/Logout",
		"https://auth.mail.ru/*",
		"https://mail.ru/*",
		"https://yandex.ru/*",
		"https://*.yandex.ru/*",
		"https://yandex.net/*",
		"https://*.yandex.net/*",
		"https://yastatic.ru/*",
		"cookies",
		"storage"
	],
	"browser_action": {
		"default_title": "One-click logout",
		"default_icon": "ocl_64.png",
		"default_popup" : "popup.html"
	},
	
	"homepage_url": "https://github.com/automatize-it/Chrome_extension_one-click_logout"
}